{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-LAB-DATAENGINEER"
		},
		"LS_DataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DataLake'"
		},
		"LSsqlserverAzure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSsqlserverAzure'"
		},
		"LS_SQL_OnPremisses_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_OnPremisses'"
		},
		"LS_DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://labbigdatesinvpsan.dfs.core.windows.net/"
		},
		"LS_KeyBigData_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keybigdatasinvpsan.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-LAB-DATAENGINEER_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_DataLake_accountKey": {
						"value": "[parameters('LS_DataLake_accountKey')]"
					},
					"LSsqlserverAzure_connectionString": {
						"value": "[parameters('LSsqlserverAzure_connectionString')]"
					},
					"LS_SQL_OnPremisses_connectionString": {
						"value": "[parameters('LS_SQL_OnPremisses_connectionString')]"
					},
					"LS_DataLake_properties_typeProperties_url": {
						"value": "[parameters('LS_DataLake_properties_typeProperties_url')]"
					},
					"LS_KeyBigData_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyBigData_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}